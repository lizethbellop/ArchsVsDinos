<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/Contracts.DTO.Result_Codes" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Contracts.DTO.Result_Codes" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="LoginResultCode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Authentication_Success" />
      <xs:enumeration value="Authentication_EmptyFields" />
      <xs:enumeration value="Authentication_InvalidCredentials" />
      <xs:enumeration value="Authentication_DatabaseError" />
      <xs:enumeration value="Authentication_UnexpectedError" />
      <xs:enumeration value="Authentication_UserBanned" />
      <xs:enumeration value="Authentication_UserAlreadyLoggedIn" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="LoginResultCode" nillable="true" type="tns:LoginResultCode" />
  <xs:simpleType name="PasswordRecoveryResult">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PasswordRecovery_Success" />
      <xs:enumeration value="PasswordRecovery_UserNotFound" />
      <xs:enumeration value="PasswordRecovery_CooldownActive" />
      <xs:enumeration value="PasswordRecovery_InvalidCode" />
      <xs:enumeration value="PasswordRecovery_CodeExpired" />
      <xs:enumeration value="PasswordRecovery_DatabaseError" />
      <xs:enumeration value="PasswordRecovery_ServerError" />
      <xs:enumeration value="PasswordRecovery_ConnectionError" />
      <xs:enumeration value="PasswordRecovery_UnexpectedError" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="PasswordRecoveryResult" nillable="true" type="tns:PasswordRecoveryResult" />
</xs:schema>