             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UniformGrid Rows="1" Columns="6">
        <controls:CardCell x:Name="CombinationCell_1" CellId="IdCombinationCell_1"/>
        <controls:CardCell x:Name="CombinationCell_2" CellId="IdCombinationCell_2"/>
        <controls:CardCell x:Name="CombinationCell_3" CellId="IdCombinationCell_3"/>
        <controls:CardCell x:Name="CombinationCell_4" CellId="IdCombinationCell_4"/>
        <controls:CardCell x:Name="CombinationCell_5" CellId="IdCombinationCell_5"/>
